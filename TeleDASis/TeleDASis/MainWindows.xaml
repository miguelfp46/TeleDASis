<Window x:Class="TeleDASis.MainWindows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeleDASis"
        mc:Ignorable="d"
        Title="MainWindows" Height="657" Width="864">
    <Grid Background="#FF3C3CEA">
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="564" Margin="132,60,-1,0" VerticalAlignment="Top" Width="725">
            <TabItem Header="Llamadas">
                <Grid Background="#FFE5E5E5" Margin="0,0,1,0">
                    <Button x:Name="button" Content="Informativa" HorizontalAlignment="Left" Margin="51,48,0,0" VerticalAlignment="Top" Width="214" Height="121" Click="button_Click"/>
                    <Button x:Name="button_Copy" Content="Emergencia" HorizontalAlignment="Left" Margin="435,282,0,0" VerticalAlignment="Top" Width="214" Height="121"/>
                    <Button x:Name="boton_reclamacion" Content="Reclamación" HorizontalAlignment="Left" Margin="434,51,0,0" VerticalAlignment="Top" Width="214" Height="121" Click="boton_reclamacion_Click"/>
                    <Button x:Name="button_Copy2" Content="Agenda" HorizontalAlignment="Left" Margin="52,280,0,0" VerticalAlignment="Top" Width="214" Height="122" Click="button_Copy2_Click"/>
                </Grid>
            </TabItem>
            <TabItem Header="Registro de Usuarios">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="23*"/>
                        <ColumnDefinition Width="727*"/>
                    </Grid.ColumnDefinitions>
                    <Button x:Name="button_Copy3" Content="Dar de Alta" HorizontalAlignment="Left" Margin="30,48,0,0" VerticalAlignment="Top" Width="214" Height="121" Grid.Column="1" Click="button_Copy3_Click"/>
                    <Button x:Name="button_Copy4" Content="Dar de Baja" HorizontalAlignment="Left" Margin="364,48,0,0" VerticalAlignment="Top" Width="214" Height="121" Grid.Column="1" Click="button_Copy3_Click"/>
                    <Button x:Name="button_Copy5" Content="Consultas" HorizontalAlignment="Left" Margin="30,255,0,0" VerticalAlignment="Top" Width="214" Height="121" Grid.Column="1" Click="button_Copy3_Click"/>
                    <Button x:Name="button_Copy6" Content="Modificaciones" HorizontalAlignment="Left" Margin="364,255,0,0" VerticalAlignment="Top" Width="214" Height="121" Grid.Column="1" Click="button_Copy3_Click"/>
                </Grid>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
        <Menu x:Name="menu" HorizontalAlignment="Left" Height="33" VerticalAlignment="Top" Width="855"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="100" Margin="20,45,0,0" VerticalAlignment="Top" Width="100"/>
        <Image x:Name="image_Copy" Source="Assets/logojpg.jpg" HorizontalAlignment="Center" Width="106" Margin="14,65,736,459"/>

    </Grid>
</Window>
